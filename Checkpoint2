float Analogin = 0;
float X = 0;
void setup() {
    pinMode(14,INPUT);
    Serial.begin(9600);
    
  }
  void loop()
  {
  
     for (int i=0;i<=5;i++)
     Analogin = 0.7* Analogin + 0.3* analogRead(A0);
     X = (Analogin * 2.5) / 4095;
     Serial.println(Analogin);
     Serial.println(X);
     delay(500);
  }
